<div class="col-md-4"></div>
  <div class="col-md-4">
    <h1>Update Contact</h1>
      <%= form_for @contact do |f| %>
        <div class="form-group">
          <%= f.text_field :first_name, placeholder: "First Name", autofocus: true, class: "form-control"%><br>
          <%= f.text_field :last_name, placeholder: "Last Name", autofocus: true, class: "form-control" %><br>
          <%= f.email_field :email, placeholder: "Email", autofocus: true, class: "form-control" %><br>
         <%= f.text_field :phone_number, placeholder: "Phone Number", autofocus: true, class: "form-control" %><br>
        <%= f.submit "Save Contact", class: "btn btn-default" %>
    <% end %>
    </div>
  </div>
<div class="col-md-4"></div>